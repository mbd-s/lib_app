<div>
  <p><strong>Library:</strong> <%= @library.name %></p>
  <p><strong>Floors:</strong> <%= @library.floor_count %></p>
  <p><strong>Area:</strong> <%= @library.floor_area %> feet<sup>2</sup></p>
  <%= button_to "Edit", edit_library_path(@library), method: :get%>
  <%= button_to "Delete", @library, method: :delete %>
</div>
